@article{0264-9381-4-5-025,
  author={R M Wald},
  title={A new type of gauge invariance for a collection of massless spin-2 fields. II. Geometrical interpretation},
  journal={Classical and Quantum Gravity},
  volume={4},
  number={5},
  pages={1279},
  url={http://stacks.iop.org/0264-9381/4/i=5/a=025},
  year={1987},
  abstract={For pt. I see ibid., vol.4, p.1267-78, 1987. A geometrical interpretation is provided for the new gauge symmetries which were found in I to be applicable to a collection of massless spin-2 fields. On a real manifold, M, the author introduces the notion of tensor fields valued in an associative commutative algebra, A, and shows that in the case where A has as identity element, most of the standard constructions and results of ordinary real-valued differential geometry concerning derivative operators, metrics and curvature can be directly carried over to the algebra-valued case.}
}

@article{0264-9381-4-5-024,
  author={C Cutler and R M Wald},
  title={A new type of gauge invariance for a collection of massless spin-2 fields. I. Existence and uniqueness},
  journal={Classical and Quantum Gravity},
  volume={4},
  number={5},
  pages={1267},
  url={http://stacks.iop.org/0264-9381/4/i=5/a=024},
  year={1987},
  abstract={It has been shown that the most general type of gauge invariance for a single massless spin-2 field is either 'normal' spin-2 gauge invariance or general covariance. The authors extend that analysis to the case of a collection of spin-2 fields. They obtain the general solution of the integrability condition which determines whether a given candidate infinitesimal symmetry arises from an exact symmetry. This general solution is a new type of gauge invariance involving associative commutative algebras in a manner analogous to the way gauge symmetries of collections of spin-1 fields are based on Lie algebras.}
}

